      <div class="empty" id="emptyState">Nu existÄƒ rezultate pentru cÄƒutarea curentÄƒ.</div>
      <div class="foot">Python â€¢ LecÈ›ia 2 â€¢ Structuri de Date</div>
    </main>
  </div>

  <script>
    (function(){
      var root = document.documentElement;

      // Theme
      var savedTheme = localStorage.getItem("py_l2_theme") || "light";
      root.setAttribute("data-theme", savedTheme);
      document.getElementById("toggleTheme").addEventListener("click", function(){
        var next = root.getAttribute("data-theme") === "dark" ? "light" : "dark";
        root.setAttribute("data-theme", next);
        localStorage.setItem("py_l2_theme", next);
      });

      var input      = document.getElementById("q");
      var clearBtn   = document.getElementById("clearSearch");
      var listEl     = document.getElementById("list");
      var cards      = Array.from(listEl.querySelectorAll(".ex"));
      var emptyState = document.getElementById("emptyState");
      var chips      = Array.from(document.querySelectorAll(".chip"));
      var currentFilter = "all";

      function normalize(s){ return (s||"").toString().toLowerCase().trim(); }

      function applyFilter(){
        var q = normalize(input.value);
        var shown = 0;
        cards.forEach(function(card){
          var topic = card.getAttribute("data-topic") || "";
          var idx   = card.getAttribute("data-idx") || "";
          var label = (card.querySelector("[data-text]")||{}).textContent || "";
          var code  = (card.querySelector("pre code")||{}).textContent || "";
          var hay   = normalize(idx + " " + label + " " + code + " " + topic);
          var topicOk = currentFilter === "all" || topic === currentFilter;
          var searchOk = !q || hay.indexOf(q) > -1;
          card.style.display = (topicOk && searchOk) ? "" : "none";
          if(topicOk && searchOk) shown++;
        });
        emptyState.style.display = shown === 0 ? "block" : "none";
      }

      // Chips filter
      chips.forEach(function(chip){
        chip.addEventListener("click", function(){
          chips.forEach(function(c){ c.classList.remove("active"); });
          chip.classList.add("active");
          currentFilter = chip.getAttribute("data-filter");
          applyFilter();
        });
      });

      input.addEventListener("input", applyFilter);
      clearBtn.addEventListener("click", function(){
        input.value = "";
        applyFilter();
        input.focus();
      });

      // Expand / Collapse
      document.getElementById("expandAll").addEventListener("click", function(){
        cards.forEach(function(c){
          if(c.style.display === "none") return;
          var d = c.querySelector("details");
          if(d) d.open = true;
        });
      });
      document.getElementById("collapseAll").addEventListener("click", function(){
        cards.forEach(function(c){
          var d = c.querySelector("details");
          if(d) d.open = false;
        });
      });

      // Copy
      listEl.addEventListener("click", function(e){
        var btn = e.target.closest("[data-copy]");
        if(!btn) return;
        var codeEl = (btn.closest(".ex")||{}).querySelector("pre code");
        var text = codeEl ? codeEl.textContent : "";
        var old = btn.innerHTML;
        if(navigator.clipboard){
          navigator.clipboard.writeText(text).then(function(){
            btn.innerHTML = "&#10003; Copiat";
            setTimeout(function(){ btn.innerHTML = old; }, 900);
          });
        }
      });

      // Progress bar - track open details
      function updateProgress(){
        var total = cards.length;
        var opened = cards.filter(function(c){
          return c.querySelector("details") && c.querySelector("details").open;
        }).length;
        document.getElementById("prog").style.width = (opened / total * 100) + "%";
        document.getElementById("progLabel").textContent = opened + " / " + total + " deschise";
      }

      listEl.addEventListener("toggle", updateProgress, true);

      applyFilter();
      updateProgress();
    })();
  </script>
</body>
